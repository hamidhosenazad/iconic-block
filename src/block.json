{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/iconic-block",
	"version": "0.1.0",
	"title": "Iconic Block",
	"category": "media",
	"icon": "smiley",
	"description": "Add icon to your site editor.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "iconic-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "left"
		},
		"label": {
			"type": "string"
		},
		"rel": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"htmlAnchor": {
			"type": "string"
		}
	}
}
